{
  "name": "rp",
  "version": "1.0.0",
  "description": "Debugging reverse proxy",
  "repository": "https://github.com/roberthardy/rp",
  "private": true,
  "scripts": {
    "test": "npm run build-test && node dist/reverseProxy/test/traceTest.js",
    "start": "node dist/reverseProxy/server.js",
    "start-dev": "concurrently npm:watch-*",
    "watch-reverse-proxy": "nodemon -w dist --ignore dist/ui dist/reverseProxy/server.js -- -i",
    "watch-ui": "npm run webpack-dev-server",
    "webpack-dev-server": "webpack-dev-server --open",
    "echo-server": "npm run build-reverse-proxy && node dist/reverseProxy/test/echoServer.js",
    "build-reverse-proxy": "tsc --project src/reverseProxy",
    "build-inspector": "webpack",
    "build-test": "npm run build-reverse-proxy"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "http-proxy": "^1.17.0",
    "minimist": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@types/connect": "^3.4.32",
    "@types/express": "^4.16.0",
    "@types/http-proxy": "^1.16.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^8.0.53",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "concurrently": "^3.6.1",
    "connect": "^3.6.6",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
