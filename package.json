{
  "name": "rp",
  "version": "1.0.0",
  "description": "Debugging reverse proxy",
  "repository": "https://github.com/roberthardy/rp",
  "main": "dist/server.js",
  "scripts": {
    "test": "npm run build && node dist/test/traceTest.js",
    "start": "npm run build && npm run watch",
    "serve": "nodemon",
    "echo-server": "npm run build && node dist/test/echoServer.js",
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"{name}\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "http-proxy": "^1.17.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/connect": "^3.4.32",
    "@types/http-proxy": "^1.16.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^8.0.53",
    "concurrently": "^3.5.0",
    "connect": "^3.6.6",
    "nodemon": "^1.17.5",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "webpack": "^4.12.1",
    "webpack-command": "^0.2.1"
  }
}
