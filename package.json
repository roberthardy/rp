{
  "name": "rp",
  "version": "1.0.0",
  "description": "Debugging reverse proxy",
  "repository": "https://github.com/roberthardy/rp",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && npm run watch",
    "serve": "nodemon",
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"{name}\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^8.0.53",
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.1",
    "typescript": "^2.6.1"
  }
}
